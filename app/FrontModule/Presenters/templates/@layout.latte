				{if $settings[18]->value}<a href="{$settings[18]->value}" target="_blank" class="twitter"><i class="bx bxl-twitter"></i></a>{/if}
				{if $settings[19]->value}<a href="{$settings[19]->value}" target="_blank" class="linkedin"><i class="bx bxl-linkedin"></i></a>{/if}
